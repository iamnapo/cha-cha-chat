{
    "name": "cha-cha-chat",
    "description": "Simple RESTful server that functions as a chat app.",
    "keywords": [
        "bunq",
        "chat",
        "framework",
        "laravel",
        "lumen"
    ],
    "license": "MIT",
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Napoleon-Christos Oikonomou",
            "email": "napoleonoikon@gmail.com",
            "homepage": "http://www.iamnapo.me",
            "role": "Developer"
        }
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "flipbox/lumen-generator": "^6.0",
        "laravel/lumen-framework": "^6.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "type": "project"
}
